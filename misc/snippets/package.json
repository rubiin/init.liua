{
  "name": "nvim-snippets",
  "author": "rubiin",
  "engines": {
    "vscode": "^1.0.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "global",
          "all"
        ],
        "path": "global.json"
      },
      {
        "language": [
          "javascript",
          "typescript"
        ],
        "path": "./nestjs.json"
      },
      {
        "language": [
          "javascriptreact",
          "typescriptreact"
        ],
        "path": "./react.json"
      },
      {
        "language": [
          "lua"
        ],
        "path": "./lua.json"
      },
      {
        "language": [
          "sh"
        ],
        "path": "./sh.json"
      }
    ]
  }
}
