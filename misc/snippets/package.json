{
  "name": "nvim-snippets",
  "author": "rubiin",
  "engines": {
    "vscode": "^1.0.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "global",
          "all"
        ],
        "path": "./snips/global.json"
      },
      {
        "language": "typescript",
        "path": "./snips/nestjs.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snips/react.json"
      },
      {
        "language": "lua",
        "path": "./snips/lua.json"
      },
      {
        "language": "sh",
        "path": "./snips/sh.json"
      }
    ]
  }
}
